{
  "name": "twint-ag/twint-shopware-plugin",
  "description": "TWINT payment plugin for Shopware 6",
  "keywords": [
    "twint",
    "twint.ch",
    "switzerland",
    "checkout",
    "regular checkout",
    "express checkout",
    "payment",
    "wordpress",
    "woocommerce",
    "woocommerce-extension"
  ],
  "version": "1.0.0",
  "type": "wordpress-plugin",
  "license": "GNU General Public License v3.0",
  "autoload": {
    "files": [
      "src/Woo/App/Helpers/helpers.php"
    ],
    "psr-4": {
      "Twint\\": "src/"
    }
  },
  "authors": [
    {
      "name": "TWINT",
      "email": "plugin@twint.ch",
      "role": "Maintainer",
      "homepage": "https://www.twint.ch/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "twint-ag/sdk": "^1.0",
    "guzzlehttp/guzzle": "^7.0",
    "chillerlan/php-qrcode": "^5.0",
    "ext-xsl": "*",
    "symfony/console": "*",
    "symfony/process": "*"
  },
  "require-dev": {
    "symfony/var-dumper": "^6.4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://git.nfq.asia/twint-ag/sdk"
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
